{
  "compilerOptions": {
    "target": "es5",
    "module": "ESNext",
    "allowJs": true,
    "jsx": "react",
    "outDir": "./dist/",
    "strict": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "paths": {
		"Atoms/*": ["./ts/components/atoms/*"],
		"Molecules/*": ["./ts/components/molecules/*"],
		"Pages/*": ["./ts/components/pages/*"]
	}
  },
  "exclude": [
    "/node_modules/**/*"
  ],
  "include": ["**/*.ts", "**/*.tsx"]
}
